{
  "name": "vThings - Device Configuration Tool",
  "version": "8.11",
  "manifest_version": 2,
  "permissions": ["serial", "storage"],
  "minimum_chrome_version": "23",
  "icons": {
    "16": "configuration16.png",
    "128": "configuration128.png",
    "48": "configuration48.png",
    "32": "configuration32.png"
  },
  "app": {
    "background": {
      "scripts": ["launch.js"],
      "transient": true
    }
  },
  "web_accessible_resources": [
   "assets/*"
  ]
}
